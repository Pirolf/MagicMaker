<p id="notice"><%= notice %></p>

<h1>Types</h1>
<% @types.each do |type| %>
  <div class='row'>
    <%= link_to 'Show', type %>
    <%= link_to 'Edit', edit_type_path(type) %>
    <%= link_to 'Destroy', type, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>

<%= link_to 'New Type', new_type_path %>
